$(function(){
    var src="";
    var _src="";
    $(".six_li li").hover(function(){
      src=$(this).find("img").attr("src");
      _src=$(this).find("img").attr("_src");
      $(this).find("img").attr("src",_src);
      _src=src;
    },function(){
      $(this).find("img").attr("src",_src);
    })
 })
